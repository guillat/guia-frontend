llo {\n  background-color: white;\n  /*clip-path: polygon(0 0, 100% 0%, 100% 75%, 36% 75%, 23% 100%, 23% 76%, 0 76%);*/\n  clip-path: polygon(0 31%, 34% 0, 29% 30%, 100% 30%, 100% 100%, 43% 100%, 0 100%);\n  color: black;\n  padding: 2em;\n}\n\n.contact {\n  font-size: 0.9em;\n  font-weight: 600;\n  text-transform: uppercase;\n  display: block;\n  margin-top: 1.2em;\n}\n\nfooter img {\n  max-width: 50px;\n}\n\n.article-title {\n  margin: 0.8em 0;\n  background-color: black;\n  padding: 0.4em;\n  opacity: 0.6;\n  display: inline;\n  line-height: 1.5;\n}\n\n.article-img {\n  float: right;\n  margin: 0.5em;\n}\n\n.source {\n  font-size: 0.8em;\n  float: right;\n}\n.block-code {\n  background-color: #e7e7e8;\n  padding: 1em;\n  margin: 1em;\n  max-width: 50%;\n}\n\n.red-button {\n  background-color: #ed3330;\n  color: white;\n  transition: background-color 0.5s ease-out, transform 0.5 ease-out;\n}\n\n.red-button:hover {\n  background-color: #023b42;\n  color: white;\n  transform: scale(1.1, 1.1);\n}\n\n.artice-image {\n  position: relative;\n  margin-top: 1em;\n}\n\n.featured-image {\n  height: 55vh;\n  object-fit: cover;\n  width: 100%;\n}\n\n.article-title {\n  position: relative;\n  width: 100%;\n  color: white;\n  bottom: 150px;\n  left: 25px;\n  \n}\n\n.category-title {\n  \n  color:#023b42;\n  border-bottom:2px solid #023b42;\n  margin:0.5em 0\n  \n  \n  \n}\n\n@media (min-width: 1200px) {\n  #hero {\n    background-image: url('../img/home-lg.jpg');\n  }\n}\n\n@media (max-width: 992px) {\n  #hero {\n    background-image: url('../img/home-md.jpg');\n  }\n \n}\n\n@media (max-width: 768px) {\n  #hero {\n    background-image: url('../img/home-sm.jpg');\n  }\n  .caixa{margin:2em 0}\n \n}\n\n@media (max-width: 576px) {\n  #hero {\n    background-image: url('../img/home-xs.jpg');\n  }\n  h1 {\n    font-size: 2em;\n  }\n  h2 {\n    margin-top: 2rem;\n    font-size: 1.5em;\n  }\n  \n}\n"},"lineCount":null}},"error":null,"hash":"b05a53f953289676d281f0f7d91b7d0c","cacheData"